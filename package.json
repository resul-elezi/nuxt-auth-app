{
  "name": "nuxt-auth-app",
  "version": "1.0.0",
  "description": "My smashing Nuxt.js project",
  "author": "Resul Elezi",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "nuxt": "^2.0.0",
    "primeicons": "^5.0.0",
    "primevue": "^2.7.0",
    "vue": "^2.7.2",
    "vue-server-renderer": "^2.7.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.2.5",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "nodemon": "^1.18.9",
    "vue-loader": "^15.9.7",
    "vue-template-compiler": "^2.7.2",
    "webpack": "^4.47.0",
    "webpack-cli": "^3.3.12"
  }
